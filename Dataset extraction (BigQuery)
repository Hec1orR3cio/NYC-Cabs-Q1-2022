#standardSQL
SELECT 
pickup_datetime, 
pickup_location_id,
dropoff_datetime,
dropoff_location_id, 
passenger_count,
trip_distance,
total_amount,
CASE WHEN payment_type = '1.0' 
       THEN 'Credit Card'
     WHEN payment_type = '2.0'
       THEN 'Cash'
     WHEN payment_type = '3.0'
       THEN 'No charge'
     WHEN payment_type = '4.0'
       THEN 'Dispute'
     WHEN payment_type = '6.0'
       THEN 'Voided Trip'
     ELSE 'Unknown'
END AS payment_type
FROM
  `bigquery-public-data.new_york_taxi_trips.tlc_green_trips_2022`
WHERE pickup_datetime >= '2022-01-01 00:00:00 UTC' 
  AND dropoff_datetime <='2022-03-31 23:59:59 UTC' 
  AND payment_type IS NOT NULL 
  AND passenger_count IS NOT NULL
ORDER BY pickup_datetime
